{
  "name": "@datadog/js-instrumentation-wasm",
  "type": "module",
  "version": "0.2.0",
  "license": "MIT",
  "description": "A library for adding Datadog instrumentation to JavaScript and TypeScript code",
  "homepage": "https://github.com/datadog/js-instrumentation-wasm#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/datadog/js-instrumentation-wasm.git"
  },
  "bugs": {
    "url": "https://github.com/datadog/js-instrumentation-wasm/issues"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "./scripts/build.sh",
    "build:tests": "./scripts/build-tests.sh",
    "clean": "./scripts/clean.sh",
    "lint": "eslint .",
    "test": "./scripts/build.sh && ./scripts/build-tests.sh && ./scripts/run-unit-tests.sh && ./scripts/run-integration-tests.sh",
    "test:integration": "./scripts/build.sh && ./scripts/build-tests.sh && ./scripts/run-integration-tests.sh",
    "test:unit": "./scripts/build.sh && ./scripts/build-tests.sh && ./scripts/run-unit-tests.sh",
    "test:unit:js": "./scripts/build.sh && ./scripts/build-tests.sh build:tests && ./scripts/run-unit-tests-js.sh",
    "test:unit:rust": "./scripts/run-unit-tests-rust.sh",
    "update:snapshots": "./scripts/update-snapshots.sh"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.2",
    "@rollup/plugin-wasm": "^6.2.2",
    "@rollup/pluginutils": "^5.1.4",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@types/node": "^22.13.1",
    "dts-buddy": "^0.6.0",
    "eslint": "^9.24.0",
    "rollup": "^4.39.0",
    "rollup-plugin-string-import": "^1.2.5",
    "tslib": "^2.8.1",
    "tsx": "^4.19.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1",
    "vite": "^6.2.5",
    "vite-plugin-commonjs": "^0.10.4",
    "vite-plugin-require": "^1.2.14",
    "vitest": "^3.0.5"
  },
  "packageManager": "yarn@4.8.1",
  "workspaces": [
    "./privacy-helpers"
  ]
}
